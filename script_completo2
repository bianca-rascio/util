basedfp_activity_reduzida_exc <- basedfp_activity_exc3[c("data","campanha","pedido","item_de_linha","tipo_line","tamanho_do_criativo","anuncio","position","conversao_de_exibicao", "conversao_de_cliques")]
basedfp_activity_reduzida_exc[is.na(basedfp_activity_reduzida_exc)] <- 0
basedfp_activity_reduzida_exc2 <-aggregate(.~ campanha +data +pedido +item_de_linha +tipo_line +tamanho_do_criativo +anuncio +position, basedfp_activity_reduzida_exc, sum, na.action = na.pass) 
###Join com impressoes
basedfp_activity_e_impressoes_exc <- full_join(basedfp_devices_ajustada2, basedfp_activity_reduzida_exc2, by = c("campanha","data","pedido", "tamanho_do_criativo", "item_de_linha", "anuncio", "tipo_line","position"))
basedfp_activity_e_impressoes_exc[is.na(basedfp_activity_e_impressoes_exc)] <- 0


