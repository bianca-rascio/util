basedfp_activity_exc <- basedfp_activity5[c("data","campanha","pedido","item_de_linha","tipo_line","anuncio", "tamanho_do_criativo","tipo_lead","position","conversao_de_exibicao", "conversao_de_cliques")]
basedfp_activity_exc2 <- filter (basedfp_activity_exc, tipo_lead == "Etapa 3 - Cupom - Sucesso")
